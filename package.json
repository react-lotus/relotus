{
  "name": "utkonos",
  "version": "1.0.0-monorepo",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/react-lotus/relotus.git"
  },
  "license": "Apache-2.0",
  "workspaces": [
    "packages/*",
    "configs/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build",
    "storybook": "nx run-many --target=storybook --configuration=composition --parallel=10",
    "build-storybook": "nx run-many --target=build-storybook --parallel=2",
    "test": "nx run-many --target test",
    "check:ts": "nx run-many --target typecheck",
    "check:lint": "nx run-many --target lint",
    "check:format": "nx format:check",
    "check:stylelint": "nx run-many --target stylelint",
    "check": "run-p --print-label check:* test",
    "fix:lint": "npm run check:lint -- --fix",
    "fix:format": "nx format:write",
    "fix:stylelint": "npm run check:stylelint -- --fix",
    "fix": "run-s --print-label fix:* ",
    "pre_release": "npm run build",
    "_release": "lerna version --yes",
    "release:version": "npm run _release --  --conventional-graduate",
    "release:rc": "npm run _release -- --conventional-prerelease",
    "release:publish": "lerna publish from-package",
    "setup": "npx check-engine && npm ci && lerna bootstrap --ci",
    "unsetup": "lerna clean -y && rm -rf node_modules",
    "prepare": "husky install"
  },
  "browserslist": [
    "last 2 Chrome version",
    "last 2 ChromeAndroid version"
  ],
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^2.3.0",
